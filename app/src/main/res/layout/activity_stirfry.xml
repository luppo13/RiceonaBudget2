<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="exosphere.alexandra.riceonabudget.Stirfry"
    android:background="@drawable/lalala"
    >

    <TextView android:text="Stir Fry" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="40sp"
        android:textColor="#fffbfbfb"
        android:textStyle="bold"
        android:id="@+id/textView33"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/beefVegCheck"
        android:layout_alignStart="@+id/beefVegCheck" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Beef and Veggie Stir Fry"
        android:id="@+id/beefVegCheck"
        android:layout_marginTop="115dp"
        android:layout_below="@+id/textView33"
        android:layout_alignRight="@+id/chickenCheck"
        android:layout_alignEnd="@+id/chickenCheck"
        android:layout_alignLeft="@+id/chickenCheck"
        android:layout_alignStart="@+id/chickenCheck"
        android:textSize="15sp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chicken and Asparagus Stir Fry"
        android:id="@+id/chickenCheck"
        android:layout_marginTop="66dp"
        android:layout_below="@+id/beefVegCheck"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="15sp" />

</RelativeLayout>
